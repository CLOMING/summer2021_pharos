  <run_depend>pharos_behavioral_planner</run_depend>
  <run_depend>pharos_bringup</run_depend>
  
  <run_depend>pharos_data_logger</run_depend>
  <run_depend>pharos_gps</run_depend>
  
  <run_depend>pharos_msgs</run_depend>
  <run_depend>pharos_path_planner</run_depend>
  
